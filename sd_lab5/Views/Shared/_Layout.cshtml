<!DOCTYPE html>
<html lang="en">
    <head>
        @await Html.PartialAsync("Partial/TagsPartial")
        
        @await Html.PartialAsync("Partial/LinksPartial")
    </head>
    <body>
        @await Html.PartialAsync("Partial/HeaderPartial")
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        @await Html.PartialAsync("Partial/FooterPartial")

        @await Html.PartialAsync("Partial/ScriptsPartial")
    </body>
</html>